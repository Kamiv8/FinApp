{
  "extends": ["airbnb","prettier","prettier/react"],
  "env": {
    "jest": true
  },
  "globals": {
    "document": true
  },
  "rules": {
    "react/jsx-filename-extension":[
      1,
      {
        "extensions":[
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/jsx-props-no-spreading": 0,
    "react/state-in-constructor": 0,
    "react/static-property-placement": 0,
    "react/prop-types": 0

  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
  }}

}